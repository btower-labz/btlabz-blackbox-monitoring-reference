{
    "name": "cratedb-repo-config",
    "schedule": "@manually",
    "timezone": "Asia/Dubai",
    "disabled": false,
    "tags": {
        "server": "true:1"
    },
    "metadata": {
        "user": "system"
    },
    "concurrency": "forbid",
    "executor": "shell",
    "executor_config": {
        "command": "bash /scripts/cratedb.sh /scripts/cratedb-repo.sql"
    },
    "retries": 15
}
